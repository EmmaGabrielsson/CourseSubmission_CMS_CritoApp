@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
		<meta name="viewport" content=" width=device-width, initial-scale=1.0"/>
        <meta name=@Model.Value("pageKeywords") content="HTML, CSS, JavaScript">
        <meta name=@Model.Value("pageDescription") content="crito agency service">
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
		<link rel="stylesheet" href="~/css/style.css" />
		<title> @Model.Value("pageTitle") | Crito - Agency Service</title>
	</head>
	<body>
	    
	    @await Html.PartialAsync("_HeaderSection")
	    @await Html.PartialAsync("_Breadcrumb")
	    <main>
	        @RenderBody()
	    </main>
	    
	    @await Html.PartialAsync("_Footer")
	    
        <!-- script section -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
